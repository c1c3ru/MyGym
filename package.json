{
  "name": "mygym-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node scripts/start-replit.js",
    "start:expo": "npx expo start --web --port 5000",
    "start:dev": "npx expo start --web --port 5000 --clear",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web --port 5000",
    "build:web": "npx expo export --platform web",
    "build:android": "node scripts/build-android.js production",
    "build:android:apk": "node scripts/build-android.js production-apk",
    "build:test": "node scripts/test-build.js",
    "build:preview": "node scripts/build-android.js preview",
    "prebuild": "npx expo prebuild --clean",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --config=jest.config.js --watch",
    "test:coverage": "jest --config=jest.config.js --coverage",
    "test:ci": "jest --config=jest.config.js --ci --coverage --watchAll=false",
    "test:unit": "TEST_LAYER=unit jest --config=tests/config/jest.config.js",
    "test:unit:domain": "TEST_LAYER=domain jest --config=tests/config/jest.config.js",
    "test:unit:data": "TEST_LAYER=data jest --config=tests/config/jest.config.js",
    "test:unit:presentation": "TEST_LAYER=presentation jest --config=tests/config/jest.config.js",
    "test:unit:infrastructure": "TEST_LAYER=infrastructure jest --config=tests/config/jest.config.js",
    "test:integration": "TEST_LAYER=integration jest --config=tests/config/jest.config.js",
    "test:integration:api": "jest tests/integration/api/",
    "test:integration:database": "jest tests/integration/database/",
    "test:integration:services": "jest tests/integration/services/",
    "test:integration:components": "jest tests/integration/components/",
    "test:e2e:legacy": "TEST_LAYER=e2e jest --config=tests/config/jest.config.js",
    "test:e2e:web": "TEST_PLATFORM=web jest tests/e2e/web/",
    "test:e2e:mobile": "TEST_PLATFORM=mobile detox test",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:all:legacy": "TEST_LAYER=all jest --config=tests/config/jest.config.js",
    "test:admin": "jest --testNamePattern='Admin'",
    "test:instructor": "jest --testNamePattern='Instructor'",
    "test:student": "jest --testNamePattern='Student'",
    "test:security": "jest tests/security/",
    "test:performance": "jest tests/performance/",
    "test:accessibility": "jest tests/accessibility/",
    "test:visual": "jest tests/visual/",
    "test:integration:emulator": "jest --config=tests/config/jest.integration.config.js",
    "test:integration:start": "firebase emulators:start --only firestore,auth --project academia-app-test",
    "test:integration:full": "npm run test:integration:start & sleep 5 && npm run test:integration:emulator && kill %1",
    "emulator:start": "firebase emulators:start --only firestore,auth --project academia-app-test",
    "emulator:ui": "firebase emulators:start --project academia-app-test",
    "test:mocks": "npm test -- tests/unit/mocks/simple-mocks.test.js",
    "test:e2e": "detox test --configuration ios.sim.debug",
    "test:e2e:build": "detox build --configuration ios.sim.debug",
    "test:all": "npm run test:basic && npm run test:mocks",
    "test:all:with-emulator": "npm run test:basic && npm run test:mocks && npm run test:integration:emulator",
    "test:all:verbose": "npm run test:basic -- --verbose && npm run test:mocks -- --verbose && npm run test:integration:emulator -- --verbose",
    "test:basic": "npm test -- tests/unit/basic/ tests/unit/domain/UserEntity.simple.test.js tests/unit/shared/utils.simple.test.js",
    "testsprite": "jest --config=testsprite.config.js",
    "testsprite:watch": "jest --config=testsprite.config.js --watch",
    "testsprite:coverage": "jest --config=testsprite.config.js --coverage",
    "testsprite:checkin": "jest --config=testsprite.config.js --testPathPattern=CheckIn.testsprite.js",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:test": "eslint src/**/*.test.js e2e/**/*.test.js src/**/*.testsprite.js",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "quality": "npm run type-check && npm run lint && npm run format:check",
    "prepare": "husky install",
    "analyze": "npx expo export --platform web && npx webpack-bundle-analyzer .expo-shared/web-build/static/js/*.js",
    "bundle-size": "npx expo export --platform web --dev false --minify true",
    "check": "npm run lint && npm run type-check",
    "setup": "node scripts/replit-setup-phase2.js",
    "clean": "npx expo r --web && npm run clean:cache",
    "clean:cache": "rm -rf .metro-cache node_modules/.cache .expo",
    "reset": "npm run clean && npm install",
    "serve:dist": "npx serve dist -l 5000",
    "start:prod": "npm run build:prod && npm run serve:dist",
    "dev": "npx expo start --web --port 5000",
    "dev:clear": "npx expo start --web --port 5000 --clear",
    "dev:tunnel": "npx expo start --tunnel",
    "build": "npx expo export --platform web --output-dir dist",
    "build:prod": "NODE_ENV=production npx expo export --platform web --output-dir dist",
    "build:analyze": "npm run build && npx bundlesize",
    "start:simple": "npx expo start --web --port 5000"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.5",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native-firebase/app": "^23.4.0",
    "@react-native-firebase/crashlytics": "^23.4.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.4.7",
    "@shopify/flash-list": "1.7.6",
    "dotenv": "^17.2.1",
    "expo": "~53.0.23",
    "expo-constants": "~17.1.7",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-device": "^7.1.4",
    "expo-file-system": "^18.1.11",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.7",
    "expo-sharing": "^13.1.5",
    "expo-status-bar": "~2.2.3",
    "firebase": "^12.2.1",
    "firebase-admin": "^13.5.0",
    "jest-expo": "~53.0.10",
    "metro-react-native-babel-transformer": "^0.77.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.63.0",
    "react-native": "0.79.5",
    "react-native-calendars": "^1.1313.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-drawer-layout": "^4.1.13",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-logs": "^5.5.0",
    "react-native-paper": "^5.14.5",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.11.2",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.20.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^2.1.1",
    "serve": "^14.2.5",
    "zod": "^3.25.76",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/cli": "^0.24.21",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@react-native-community/cli": "^20.0.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.2",
    "@testsprite/testsprite-mcp": "^0.0.13",
    "@types/jest": "^29.5.8",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-define": "^2.1.4",
    "detox": "^20.13.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lint-staged": "^15.2.2",
    "playwright": "^1.55.1",
    "prettier": "^3.2.5",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-chart-kit",
          "@react-native-community/eslint-config",
          "dotenv",
          "firebase",
          "firebase-admin",
          "metro-react-native-babel-transformer",
          "react-native-vector-icons",
          "react-virtualized-auto-sizer",
          "react-window",
          "serve"
        ]
      }
    }
  },
  "private": true
}
